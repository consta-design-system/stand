  "scripts": {
    "postinstall": "husky install",
    "tsc-dry-run": "tsc --noEmit",
    "lint:js": "eslint --ext .js,.jsx,.tsx,.ts --fix . --quiet",
    "lint:css": "stylelint ./src/**/*.css --fix",
    "lint": "yarn run lint:js && yarn run lint:css",
    "format:svg": "prettier --write '**/*.svg' --parser html",
    "pre-push": "rm -rf ./repositories && rm -rf src/stands && yarn run tsc-dry-run",
    "pre-commit": "yarn run lint-staged",
    "pre-build": "node builder/preBuild.js --config=./build.config.js",
    "test": "rm -rf src/stands && yarn tsc-dry-run",
    "unit": "jest",
    "unit:watch": "jest --watch",
    "unit:clear": "jest --clearCache",
    "build:css": "NODE_ENV=buildProduction postcss 'src/**/*.css' --base src --dir ./dist/src",
    "build:js": "NODE_ENV=buildProduction BUILDING_JS=true babel --extensions '.ts,.tsx' --config-file ./babel.config.js --source-maps --out-dir dist/src ./src",
    "build:types": "NODE_ENV=buildProduction ttsc --declaration --outDir dist/src/ --emitDeclarationOnly -p tsconfig.build.json",
    "build:package.json": "node scripts/prepare-package-json.js",
    "build:templates": "cp -R templates dist/templates",
    "build:webpack.config.js": "cp webpack.config.js dist/webpack.config.js",
    "build:public": "cp -R public dist/public",
    "build:index": "cp index.js dist/index.js && cp index.d.ts dist/index.d.ts",
    "build:scripts": "cp -R scripts dist/scripts",
    "build:docs": "cp README.md dist/README.md",
    "build:clear": "rm -rf dist/src/stands",
    "build:copy-assets": "node scripts/copyAssets.js",
    "build": "rm -rf ./dist && rm -rf ./src/stands && yarn build:css && yarn build:js && yarn build:types && yarn build:package.json && yarn build:scripts && yarn build:templates && yarn build:webpack.config.js && yarn build:docs && yarn build:public && yarn build:index && yarn build:copy-assets && yarn build:clear",
    "stylelint-fix": "stylelint --fix",
    "clone-repos": "sh ./__mocks__/clone-repositories.sh",
    "stand:generate": "node ./scripts/standGenerate.js --config=__mocks__/config.js",
    "stand:build": "rm -rf ./build && yarn stand:generate && NODE_ENV=production webpack --mode production --config webpack.mocks.config.js",
    "stand:start": "NODE_ENV=development webpack serve --open --progress --mode development --hot --progress --stats-error-details --config webpack.mocks.config.js",
    "copy-deps": "node ./__mocks__/copy-deps.js",
    "start": "yarn clone-repos && yarn stand:generate && yarn stand:start"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableScopeLowerCase": true,
      "types": {
        "feat": {
          "description": "Adding new feature"
        },
        "fix": {
          "description": "Bug fix"
        },
        "refactor": {
          "description": "Editing code without fixing bugs or adding new features"
        },
        "docs": {
          "description": "Documentation Update"
        },
        "chore": {
          "description": "Add / update / customize tools and libraries"
        },
        "changelog": {
          "description": "Add / update / changelog"
        },
        "release": {
          "description": "New release"
        }
      }
    }
